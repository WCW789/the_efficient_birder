<div id="<%= dom_id bird %>">
  <p>
    <strong>User:</strong>
    <%= bird.user_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= bird.name %>
  </p>

  <p>
    <strong>Datetime:</strong>
    <%= bird.datetime %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= bird.notes %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= bird.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= bird.longitude %>
  </p>

</div>
