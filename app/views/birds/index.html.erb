<div class="actions centered">
  <%= link_to "New Bird", birds_photo_path, class: "new_bird_button" %>
</div>
<br>

<h1>List of Birds</h1>

<div class="actions centered">
  <%= link_to "Export Birding Journal", birds_export_path, class: "export_button" %>
</div>
<br>

<div id="birds">
  <% @birds.each do |bird| %>
    <%= render bird %>
    <%= link_to "Edit/Delete", bird, class: "centered" %>
    <br><br><br>
  <% end %>
</div>

<br>
<div class="centered">
  <%= paginate @birds %>
</div>
